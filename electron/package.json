{
  "name": "electron-react-app",
  "productName": "electron-react-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron . --disable-gpu-vsync 2>/dev/null\"",
    "build": "vite build && electron-builder",
    "build:linux": "vite build && electron-builder --linux",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.sibirajmunirathinam.examportal",
    "productName": "ExamPortal",
    "directories": {
      "output": "release"
    },
    "files": [
      "**/*",
      "!node_modules/**",
      "!release/**",
      "!src/**",
      "!*.config.*",
      "!*.md"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Education"
    }
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "axios": "^1.7.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "^7.1.0",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.0",
    "tailwindcss": "^4.0.0",
    "vite": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
